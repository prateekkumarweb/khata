{
  "name": "khata-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-s build-only type-check",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint .",
    "lint": "run-s lint:*",
    "fmt": "prettier --check src/",
    "fmt:fix": "prettier --write src/",
    "gen:routes": "node extract-routes.js ../khata-service/routes.gen.txt",
    "gen:api": "openapi-typescript http://localhost:8000/openapi.json -o src/lib/openapi.gen.ts",
    "gen:api-file": "openapi-typescript ./openapi.gen.json -o src/lib/openapi.gen.ts"
  },
  "dependencies": {
    "@nuxt/ui": "^3.3.3",
    "@pinia/colada": "^0.17.3",
    "@pinia/colada-plugin-retry": "^0.1.0",
    "@tanstack/vue-table": "^8.21.3",
    "@unhead/vue": "^2.0.14",
    "@unovis/vue": "1.6.0",
    "@vueuse/core": "^13.9.0",
    "dayjs": "^1.11.18",
    "openapi-fetch": "^0.14.0",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.35.0",
    "eslint-plugin-oxlint": "~1.14.0",
    "eslint-plugin-vue": "~10.4.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "openapi-typescript": "^7.9.1",
    "oxlint": "~1.14.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.9.2",
    "unplugin-vue-router": "^0.15.0",
    "vite": "npm:rolldown-vite@7.1.0",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vue-tsc": "^3.0.6"
  }
}
