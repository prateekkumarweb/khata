{
  "name": "twopi-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "oxlint && eslint .",
    "gen:routes": "tsr generate && cat src/routeTree.gen.ts | rg -oUP '(?<=ROUTE_MANIFEST_START)(?s).*(?=ROUTE_MANIFEST_END)' | jq -r '.routes | keys[] | gsub(\"\\\\$(?<a>[[:alpha:]]+)\"; \"{\\(.a)}\")' > ../twopi-service/routes.gen.txt",
    "typecheck": "tsr generate && tsc -b",
    "gen:api": "openapi-typescript http://localhost:8000/openapi.json -o src/lib/openapi.gen.ts",
    "gen:api-file": "openapi-typescript ./openapi.gen.json -o src/lib/openapi.gen.ts"
  },
  "dependencies": {
    "@tanstack/solid-query": "^5.68.0",
    "@tanstack/solid-router": "^1.114.23",
    "openapi-fetch": "^0.13.5",
    "solid-js": "^1.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@tanstack/router-cli": "^1.114.23",
    "@tanstack/router-plugin": "^1.114.23",
    "@tanstack/solid-query-devtools": "^5.68.0",
    "@tanstack/solid-router-devtools": "^1.114.23",
    "@typescript-eslint/parser": "^8.26.1",
    "clsx": "^2.1.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-oxlint": "^0.16.0",
    "eslint-plugin-solid": "^0.14.5",
    "globals": "^16.0.0",
    "openapi-typescript": "^7.6.1",
    "oxlint": "^0.16.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.14",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vite-plugin-solid": "^2.11.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
