{
  "name": "twopi-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "oxlint && eslint .",
    "gen:routes": "tsr generate && cat src/routeTree.gen.ts | rg -oUP '(?<=ROUTE_MANIFEST_START)(?s).*(?=ROUTE_MANIFEST_END)' | jq -r '.routes | keys[] | gsub(\"\\\\$(?<a>[[:alpha:]]+)\"; \"{\\(.a)}\")' > ../twopi-service/routes.gen.txt",
    "typecheck": "tsr generate && tsc -b",
    "gen:api": "openapi-typescript http://localhost:8000/openapi.json -o src/lib/openapi.gen.ts",
    "gen:api-file": "openapi-typescript ./openapi.gen.json -o src/lib/openapi.gen.ts"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.9",
    "@solid-primitives/storage": "^4.3.1",
    "@tanstack/solid-form": "^1.3.0",
    "@tanstack/solid-query": "^5.71.10",
    "@tanstack/solid-router": "^1.114.33",
    "@tanstack/solid-table": "^8.21.2",
    "@tanstack/zod-adapter": "^1.114.34",
    "class-variance-authority": "^0.7.1",
    "lucide-solid": "^0.487.0",
    "openapi-fetch": "^0.13.5",
    "solid-js": "^1.9.5",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "^4.1.3",
    "tw-animate-css": "^1.2.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/eslint-plugin-query": "^5.71.5",
    "@tanstack/router-cli": "^1.114.34",
    "@tanstack/router-plugin": "^1.114.34",
    "@tanstack/solid-query-devtools": "^5.71.10",
    "@tanstack/solid-router-devtools": "^1.114.33",
    "@typescript-eslint/parser": "^8.29.0",
    "clsx": "^2.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-oxlint": "^0.16.4",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^16.0.0",
    "openapi-typescript": "^7.6.1",
    "oxlint": "^0.16.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "solid-devtools": "^0.34.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-plugin-solid": "^2.11.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
