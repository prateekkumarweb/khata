{
  "name": "twopi-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "oxlint && eslint .",
    "gen:routes": "tsr generate && cat src/routeTree.gen.ts | rg -oUP '(?<=ROUTE_MANIFEST_START)(?s).*(?=ROUTE_MANIFEST_END)' | jq -r '.routes | keys[] | gsub(\"\\\\$(?<a>[[:alpha:]]+)\"; \"{\\(.a)}\")' > ../twopi-service/routes.gen.txt",
    "typecheck": "tsr generate && tsc -b",
    "gen:api": "openapi-typescript http://localhost:8000/openapi.json -o src/lib/openapi.gen.ts",
    "gen:api-file": "openapi-typescript ./openapi.gen.json -o src/lib/openapi.gen.ts"
  },
  "dependencies": {
    "@kobalte/core": "^0.13.9",
    "@solid-primitives/storage": "^4.3.2",
    "@tanstack/solid-form": "^1.12.0",
    "@tanstack/solid-query": "^5.77.2",
    "@tanstack/solid-router": "^1.120.10",
    "@tanstack/solid-table": "^8.21.3",
    "@tanstack/solid-virtual": "^3.13.9",
    "@tanstack/zod-adapter": "^1.120.11",
    "class-variance-authority": "^0.7.1",
    "dayjs": "^1.11.13",
    "lucide-solid": "^0.511.0",
    "marked": "^15.0.12",
    "openapi-fetch": "^0.14.0",
    "solid-js": "^1.9.7",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.0",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@kobalte/tailwindcss": "^0.9.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/eslint-plugin-query": "^5.78.0",
    "@tanstack/router-cli": "^1.120.11",
    "@tanstack/router-plugin": "^1.120.11",
    "@tanstack/solid-query-devtools": "^5.77.2",
    "@tanstack/solid-router-devtools": "^1.120.10",
    "@typescript-eslint/parser": "^8.33.0",
    "clsx": "^2.1.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-oxlint": "^0.16.12",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^16.2.0",
    "openapi-typescript": "^7.8.0",
    "oxlint": "^0.16.12",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "solid-devtools": "^0.34.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "^6.3.5",
    "vite-plugin-solid": "^2.11.6",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
