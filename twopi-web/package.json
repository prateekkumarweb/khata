{
  "name": "twopi-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "oxlint && eslint .",
    "gen:routes": "tsr generate && cat src/routeTree.gen.ts | rg -oUP '(?<=ROUTE_MANIFEST_START)(?s).*(?=ROUTE_MANIFEST_END)' | jq -r '.routes | keys[] | gsub(\"\\\\$(?<a>[[:alpha:]]+)\"; \"{\\(.a)}\")' > ../twopi-service/routes.gen.txt",
    "typecheck": "tsr generate && tsc -b",
    "gen:api": "openapi-typescript http://localhost:8000/openapi.json -o src/lib/openapi.gen.ts",
    "gen:api-file": "openapi-typescript ./openapi.gen.json -o src/lib/openapi.gen.ts"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tanstack/react-form": "^1.0.0",
    "@tanstack/react-query": "^5.67.1",
    "@tanstack/react-router": "^1.112.17",
    "@tanstack/react-table": "^8.21.2",
    "@tanstack/zod-adapter": "^1.112.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.0.4",
    "dayjs": "^1.11.13",
    "lucide-react": "^0.477.0",
    "openapi-fetch": "^0.13.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.11",
